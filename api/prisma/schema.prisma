generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model Task{
    id String @id @default(uuid())
    title       String
    done        Boolean   @default(false)
    priority    Int
    createdAt   DateTime  @default(now())
}
